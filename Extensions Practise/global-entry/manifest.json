{
  "manifest_version": 3,
  "name": "Global Entry Drops",
  "description": "Find global entries",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/favicon-16.png",
    "32": "images/favicon-32.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "alarms", "notifications"]
}
