{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "An extension that blocks websites",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.ts"]
    }
  ],
  "permissions": ["tabs"]
}
